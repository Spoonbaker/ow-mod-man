{
    "name": "owmods_gui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "devtools": "react-devtools --in-process-gpu",
        "gen-types": "typeshare ../.. --lang=typescript --output-file=src/types.d.ts",
        "build": "tsc && vite build --emptyOutDir",
        "preview": "vite preview",
        "check": "pnpm lint && pnpm prettify",
        "lint": "eslint --cache . --fix",
        "lint-dry": "eslint .",
        "prettify": "prettier --write . --plugin-search-dir=. --ignore-unknown",
        "prettier-check": "prettier --check . --plugin-search-dir=. --ignore-unknown"
    },
    "dependencies": {
        "@picocss/pico": "^1.5.9",
        "@tauri-apps/api": "^1.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.8.0",
        "react-virtualized-auto-sizer": "^1.0.15",
        "react-window": "^1.8.9",
        "tauri-plugin-fs-watch-api": "github:tauri-apps/tauri-plugin-fs-watch"
    },
    "devDependencies": {
        "@types/react": "^18.0.37",
        "@types/react-dom": "^18.0.11",
        "@types/react-virtualized-auto-sizer": "^1.0.1",
        "@types/react-window": "^1.8.5",
        "@typescript-eslint/eslint-plugin": "^5.59.0",
        "@typescript-eslint/parser": "^5.59.0",
        "@vitejs/plugin-react": "^3.1.0",
        "eslint": "^8.38.0",
        "eslint-plugin-react": "^7.32.2",
        "prettier": "^2.8.7",
        "sass": "^1.62.0",
        "typescript": "^4.9.5",
        "vite": "^4.2.2",
        "vite-plugin-html": "^3.2.0"
    }
}
